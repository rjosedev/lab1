hostname name # SW_CORE_1

enable secret password # Mypass123!
username utnfrc privilege 15 secret 5 encrypted_password

no ip domain lookup
ip domain name mydomain.local

crypto key generate rsa general-keys modulus 4096

crypto key generate rsa modulus 4096
crypto key generate rsa label sshkeys
ip ssh rsa keypair-name sshkeys

interface GigabitEthernet0/0
 description ### Link to Cloud0 - Mgmt/OOB ###
 no switchport
 ip address 192.168.100.201 255.255.255.0
 negotiation auto
 no shutdown

line aux 0
 exec-timeout 0 1
 no exec
 transport output none
line vty 0 15
 exec-timeout 20 0
 login local
 transport input ssh

ip ssh time-out 30
ip ssh source-interface GigabitEthernet0/0
ip ssh rsa keypair-name sshkeys
ip ssh version 2

