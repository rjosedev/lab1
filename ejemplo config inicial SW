hostname SW_CORE_1

enable secret Mypass123!
do sho run | inc ena
username utnfrc privilege 15 secret 5

no ip domain lookup
ip domain name mydomain.local

crypto key generate rsa general-keys modulus 4096

crypto key generate rsa modulus 4096
crypto key generate rsa label sshkeys
ip ssh rsa keypair-name sshkeys

interface GigabitEthernet0/0
 description ### Connect to Cloud0 - VLAN 1 Mgmt/OOB ###
 no shutdown
 exit
inteface vlan 1
 description ### Connect to VTYs - VLAN 1 Mgmt/OOB ###
 ip address 192.168.100.200 255.255.255.0
 no shutdown
 exit
 
line aux 0
 exec-timeout 0 1
 no exec
 transport output none
line vty 0 4
 exec-timeout 20 0
 login local
 transport input ssh

ip ssh time-out 30
ip ssh source-interface GigabitEthernet0/0
ip ssh rsa keypair-name sshkeys
ip ssh version 2

